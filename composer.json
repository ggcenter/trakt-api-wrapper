{
  "name": "trakt-php-api/trakt",
  "license": "GPL-3.0-only",
  "description": "Trakt API Wrapper",
  "authors": [
    {
      "name": "megawubs",
      "email": "bwubs@me.com"
    },
    {
      "name": "pvguerra",
      "email": "paulovictorguerra@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "~5.6",
    "nesbot/carbon": "~1.2",
    "guzzlehttp/guzzle": "~6.3.0",
    "league/oauth2-client": "~2.3.0",
    "league/uri": "^5.2.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.4.0",
    "mockery/mockery": "^1.0",
    "symfony/console": "^4.0.4",
    "league/flysystem": "^1.0.9",
    "symfony/var-dumper": "v4.0.4",
    "psr/log": "^1.0.2",
    "phpunit/phpunit": "^7.0.1"
  },
  "autoload": {
    "psr-0": {
      "TraktPHPApi": "src/"
    },
    "classmap": [
      "tests"
    ]
  },
  "bin": [
    "bin/trakt"
  ],
  "config": {
    "bin-dir": "scripts"
  },
  "extra": {
    "laravel": {
      "providers": [
        "TraktPHPApi\\Trakt\\Providers\\Laravel\\TraktApiServiceProvider"
      ],
      "aliases": {
        "TraktApi": "TraktPHPApi\\Trakt\\Facades\\TraktApi"
      }
    }
  }
}
